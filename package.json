{
  "name": "audio-container-parser",
  "version": "0.2.0",
  "description": "Node.js TypeScript library that parses audio frames from common audio containers and exposes them.",
  "author": "vlxdisluv",
  "license": "MIT",
  "private": false,
  "type": "module",
  "keywords": ["audio", "wav", "ogg", "opus", "pcm", "frames", "stream", "typescript", "node"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vlxdisluv/audio-container-parser.git"
  },
  "bugs": { "url": "https://github.com/vlxdisluv/audio-container-parser/issues" },
  "homepage": "https://github.com/vlxdisluv/audio-container-parser#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepack": "npm run build",
    "ex:opus": "tsx examples/stream-opus.ts",
    "ex:wav": "tsx examples/stream-wav.ts",
    "ws:l16":  "FILE=examples/assets/sample-speech.l16.16k.wav CODEC=l16  dotenv -e .env -- tsx examples/ws/ws-stream-wav.ts",
    "ws:pcmu": "FILE=examples/assets/sample-speech.pcmu.8k.wav CODEC=pcmu dotenv -e .env -- tsx examples/ws/ws-stream-wav.ts",
    "ws:pcma": "FILE=examples/assets/sample-speech.pcma.8k.wav CODEC=pcma dotenv -e .env -- tsx examples/ws/ws-stream-wav.ts"
  },
  "dependencies": {
    "opus-decoder": "^0.7.10"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/ws": "^8.5.10",
    "dotenv-cli": "^11.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.9.3",
    "ws": "^8.18.0"
  },
  "engines": {
    "node": ">=18"
  }
}
